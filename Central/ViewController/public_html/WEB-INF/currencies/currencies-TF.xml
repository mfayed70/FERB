<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="currencies-TF">
    <template-reference>
      <document>/WEB-INF/page-gragment-template.xml</document>
      <id>page-gragment-template</id>
    </template-reference>
    <default-activity>Execute</default-activity>
    <method-call id="Execute">
      <method>#{bindings.Execute.execute}</method>
      <outcome>
        <fixed-outcome>Execute</fixed-outcome>
      </outcome>
    </method-call>
    <view id="currencies">
      <page>/WEB-INF/currencies/currencies.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>Execute</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Execute</from-outcome>
        <to-activity-id>currencies</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
