<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CurrMonthLOV"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="JDBC"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.aalm.master.model.lovs.CurrMonthLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT 
    MONTHNAME(MAKEDATE(YEAR(CURDATE()), n.n * 30)) AS month_name
FROM (
   SELECT 1 AS n UNION ALL SELECT 2 UNION ALL SELECT 3 UNION ALL SELECT 4
   UNION ALL SELECT 5 UNION ALL SELECT 6 UNION ALL SELECT 7 UNION ALL SELECT 8
   UNION ALL SELECT 9 UNION ALL SELECT 10 UNION ALL SELECT 11 UNION ALL SELECT 12
) n
GROUP BY n.n
ORDER BY n.n]]></SQLQuery>
  <ViewAttribute
    Name="MonthName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="month_name"
    Expression="month_name"
    SQLType="VARCHAR"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="MonthName"/>
  </AttrArray>
  <ResourceBundle>
    <XliffBundle
      id="com.aalm.master.model.AalmMasterMBndl"/>
  </ResourceBundle>
</ViewObject>
